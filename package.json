{
  "name": "webpack_scss",
  "version": "1.0.0",
  "description": "Template: Webpack + Babel + SCSS",
  "main": "index.js",
  "scripts": {
    "start": "yarn run build",
    "build": "webpack --mode=development && cp src/index.html dist/index.html && webpack-dev-server --content-base src/ --inline --hot --port 3001",
    "build:prod": "webpack --mode=production && cp src/index.html dist/index.html"
  },
  "keywords": [
    "template, webpack, SCSS"
  ],
  "author": "SVO",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "node-sass": "^4.8.3",
    "sass-loader": "^7.0.1",
    "webpack": "^4.6.0",
    "webpack-dev-server": "^3.1.3"
  }
}
